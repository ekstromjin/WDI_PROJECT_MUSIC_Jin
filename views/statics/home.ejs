<div class="container home-users">
    <div class="row">
        <% users.forEach(user => { %>
            <div class="col-md-3 col-lg-2 col-sm-4 col-xs-12 home-users-item">
                <a id="usrlnk" href="/users/<%= user.id %>">
                    <div class="home-users-item-img">
                        <img id="profile" src="<%= user.photo %>">
                    </div>
                    <div class="home-users-item-name"><p id="usrnm"><%= user.firstName %></p></div>
                </a>
            </div>
        <% }) %>
    </div>
</div>
